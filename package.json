{
  "name": "esri-leaflet-webpack",
  "version": "1.0.0",
  "description": "A simple starter template for building an app with webpack, esri-leaflet, and React.js.",
  "main": "index.js",
  "scripts": {
    "start": "ENV=dev webpack -p -d --watch",
    "build": "rm -rf build && ENV=prod webpack -p && cp ./src/{.htaccess,index.html} ./build/ && ditto src/libs/*.js ./build/libs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Robert-W/esri-leaflet-webpack.git"
  },
  "keywords": [
    "esri",
    "leaflet",
    "map",
    "javascript",
    "webpack",
    "react",
    "jsx"
  ],
  "author": "Robert Winterbottom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Robert-W/esri-leaflet-webpack/issues"
  },
  "homepage": "https://github.com/Robert-W/esri-leaflet-webpack",
  "dependencies": {
    "css-loader": "^0.9.0",
    "file-loader": "^0.8.1",
    "jsx-loader": "^0.12.2",
    "nib": "^1.0.4",
    "style-loader": "^0.8.2",
    "stylus": "^0.49.3",
    "stylus-loader": "^0.5.0",
    "url-loader": "^0.5.5"
  }
}
