{
  "name": "esri-leaflet-webpack",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run watch",
    "watch": "npm run jade & npm run stylus & ENV=dev webpack -p -d --watch",
    "dist": "rm -rf dist && npm run stylus:dist && npm run jade:dist && ENV=prod webpack -p",
    "stylus": "stylus src/css/base.styl -l --watch",
    "jade": "jade src/index.jade -O src/index.json -o build --watch",
    "stylus:dist": "stylus src/css/base.styl --compress",
    "jade:dist": "jade src/index.jade -O src/index.json -o dist"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.6.15",
    "babel-loader": "^5.3.1",
    "jade": "^1.11.0",
    "node-libs-browser": "^0.5.2",
    "stylus": "^0.51.1",
    "webpack": "^1.10.1"
  }
}
